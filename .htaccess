<IfModule mod_rewrite.c>
  Options +FollowSymlinks -Multiviews
  RewriteEngine On
  RewriteBase /
  RewriteCond %{REQUEST_FILENAME} !-f

  # Temporarily moved pages version 1
    RewriteRule ^services$ /error-302 [R=302,L]

  # Permanent redirects for version 3
    RewriteRule ^about=contact$ /contact [R=301]
    RewriteRule ^about=impressum$ /imprint [R=301]
    RewriteRule ^information=introduction$ /activities [R=301]

  # Temporarily moved pages (error 302)
    RewriteRule ^information=services$ /error-302 [R=302,L]

  # Explicit rewrite rules for root positioned pages
    RewriteRule ^contact$ /proof.php?file=contact [QSA,L]
    RewriteRule ^error-([a-zA-Z0-9\-]+)$ /error.php?file=$1 [QSA,L]
    RewriteRule ^imprint$ /proof.php?file=imprint [QSA,L]
    RewriteRule ^activities$ /stall.php?file=activities [QSA,L]
    RewriteRule ^principles$ /stall.php?file=principles [QSA,L]
</IfModule>

ErrorDocument 302 /error-302
ErrorDocument 404 /error-404
